

def string_to_int(s):

    try:
        return int(s)
    except ValueError:
        return f'ОШИБКА: невозможно преобразовать "s" в целое число'



def read_file(filename):
    try:
        with open(filename, 'r') as file:
            return file.read()
    except FileNotFoundError:
        return f'ошибка: файл  не найден'
    except IOError:
        return f'ошибка ввода/вывода при работе с файлом'




def divide_numbers(a,b):
    try:
        return a/b
    except ZeroDivisionError:
        return f'ОШИБКА: на ноль делить нельзя'
    except TypeError:
        return f'ОШИБКА: аргументы не числа'




def access_list_element(lst, index):

    try:
        return lst[index]
    except IndexError:
        return f'ОШИБКА: индекс {index} вне диапозона списка'
    except TypeError:
        return f'ОШИБКА: индекс должен быть целым числом'


